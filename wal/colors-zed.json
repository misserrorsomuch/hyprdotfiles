{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#070e1090",
                "editor.background": "#070e1090",
                "editor.foreground": "#c1c2c3",
                "text": "#c1c2c3",
                "text.muted": "#c1c2c370",
                "text.ignored": "#c1c2c340",
                "text.placeholder": "#c1c2c350",
                "ignored": "#c1c2c330",
                "element.hover": "#c1c2c330",
                "ghost_element.hover": "#053b7830",
                "ghost_element.selected": "#053b7830",
                "ghost_element.active": "#053b7860",
                "border": "#070e10",
                "editor.highlighted_line.background": "#053b7810",
                "editor.active_line.background": "#053b7810",
                "panel.background": "#070e1090",
                "title_bar.background": "#070e1090",
                "title_bar.inactive_background": "#070e1090",
                "status_bar.background": "#070e1090",
                "drop_target.background": "#070e1090",
                "elevated_surface.background": "#070e10",
                "toolbar.background": "#070e1090",
                "tab_bar.background": "#070e1090",
                "tab.inactive_background": "#070e1090",
                "tab.active_background": "#053b7830",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#070e10",
                "scrollbar.thumb.background": "#053b78",
                "editor.gutter.background": "#070e1090",
                "terminal.background": "#070e1010",
                "terminal.foreground": "#c1c2c3",
                "terminal.dim_foreground": "#c1c2c3",
                "terminal.bright_foreground": "#c1c2c3",
                "terminal.ansi.black": "#070e10",
                "terminal.ansi.red": "#053b78",
                "terminal.ansi.green": "#06717e",
                "terminal.ansi.yellow": "#166dba",
                "terminal.ansi.blue": "#4374c7",
                "terminal.ansi.magenta": "#149b92",
                "terminal.ansi.cyan": "#52a7be",
                "terminal.ansi.white": "#c1c2c3",
                "terminal.ansi.bright_black": "#566669",
                "terminal.ansi.bright_red": "#053b78",
                "terminal.ansi.bright_green": "#06717e",
                "terminal.ansi.bright_yellow": "#166dba",
                "terminal.ansi.bright_blue": "#4374c7",
                "terminal.ansi.bright_magenta": "#149b92",
                "terminal.ansi.bright_cyan": "#52a7be",
                "terminal.ansi.bright_white": "#c1c2c3",
                "modified": "#053b78",
                "syntax": {
                    "attribute": {
                        "color": "#053b78"
                    },
                    "boolean": {
                      "color": "#06717e"
                    },
                    "comment": {
                      "color": "#566669"
                    },
                    "comment.doc": {
                      "color": "#566669"
                    },
                    "constant": {
                      "color": "#06717e"
                    },
                    "function": {
                      "color": "#52a7be"
                    },
                    "keyword": {
                      "color": "#4374c7"
                    },
                    "number": {
                      "color": "#149b92"
                    },
                    "operator": {
                      "color": "#4374c7"
                    },
                    "string": {
                      "color": "#053b78"
                    },
                    "variable": {
                      "color": "#06717e"
                    }
                },
                "players": [
                  {
                    "cursor": "#053b78",
                    "background": "#070e10",
                    "selection": "#053b7830"
                  }
                ]
            }
        }
    ]
}
